    "react": "18.1.0",
    "react-native": "0.70.5"
  },
  "devDependencies": {
    "@babel/core": "7.20.2",
    "@types/react": "18.0.25",
    "@types/react-native": "0.70.6",
    "eslint": "8.27.0",
    "eslint-config-universe": "11.1.1",
    "prettier": "2.7.1",
    "ts-node": "10.9.1",
    "typescript": "4.6.3"
  },
  "private": true
}
